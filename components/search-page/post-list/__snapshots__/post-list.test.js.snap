                  "value": Array [
                    Object {
                      "styles": Array [],
                      "type": "Text",
                      "value": "The president of the United States may be an ",
                    },
                    Object {
                      "reference": "https://www.washingtonpost.com/news/morning-mix/wp/2017/01/12/shaking-hands-is-barbaric-donald-trump-the-germaphobe-in-chief",
                      "type": "Link",
