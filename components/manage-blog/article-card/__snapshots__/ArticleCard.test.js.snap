          "caption": Array [
            Object {
              "styles": Array [],
              "type": "Text",
              "value": "The man, the myth, ",
            },
            Object {
              "styles": Array [
                "Italic",
              ],
              "type": "Text",
              "value": "The Meg.",
            },
          ],
          "format": "jpg",
          "height": 1200,
